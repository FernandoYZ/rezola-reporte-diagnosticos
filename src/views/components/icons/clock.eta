<svg
    xmlns="http://www.w3.org/2000/svg"
    width="<%= it.size ?? 24 %>"
    height="<%= it.size ?? 24 %>"
    viewBox="0 0 24 24"
    fill="none"
    stroke="currentColor"
    stroke-width="<%= it.strokeWidth ?? 2 %>"
    stroke-linecap="round"
    stroke-linejoin="round"
    class="lucide lucide-clock <%= it.class ?? 'w-6 h-6' %>"
    aria-label="<%= it.ariaLabel ?? 'Clock' %>"
>
    <circle cx="12" cy="12" r="10"/>
    <polyline points="12 6 12 12 16 14"/>
</svg>
